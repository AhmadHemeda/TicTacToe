<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane id="AnchorPane" prefHeight="861.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="tictactoe.HistoryTwoPlController">
   <children>
      <GridPane prefHeight="861.0" prefWidth="1200.0">
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="523.0" minWidth="523.0" prefWidth="523.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="677.0" minWidth="677.0" prefWidth="677.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
         <children>
            <ImageView fitHeight="861.0" fitWidth="523.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Resources/images/background.png" />
               </image>
            </ImageView>
            <GridPane GridPane.columnIndex="1">
               <columnConstraints>
                  <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
               </columnConstraints>
               <rowConstraints>
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="800.0" minHeight="800.0" prefHeight="800.0" vgrow="SOMETIMES" />
               </rowConstraints>
               <children>
                  <TableView fx:id="STable" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #f3eeee; -fx-border-radius: 50; -fx-border-style: F0C48A;" GridPane.rowIndex="1">
                     <columns>
                        <TableColumn fx:id="idCol" maxWidth="100.0" minWidth="100.0" prefWidth="100.0" text="ID" />
                        <TableColumn fx:id="playerx" maxWidth="105.0" minWidth="105.0" prefWidth="105.0" text="X" />
                        <TableColumn fx:id="playery" maxWidth="105.0" minWidth="105.0" prefWidth="105.0" text="Y" />
                        <TableColumn fx:id="winnerCol" maxWidth="102.0" minWidth="102.0" prefWidth="102.0" text="Winner" />
                        <TableColumn fx:id="dateCol" maxWidth="102.0" minWidth="102.0" prefWidth="102.0" text="Date" />
                        <TableColumn fx:id="timeCol" maxWidth="102.0" minWidth="102.0" prefWidth="102.0" text="Time" />
                     </columns>
                     <GridPane.margin>
                        <Insets bottom="50.0" left="30.0" right="30.0" top="10.0" />
                     </GridPane.margin>
                  </TableView>
                  <Group fx:id="back" onMouseClicked="#backFromHist">
                     <children>
                        <ImageView fitHeight="30.0" fitWidth="30.0" pickOnBounds="true" preserveRatio="true">
                           <image>
                              <Image url="@../Resources/images/backTwoPlayers.png" />
                           </image>
                        </ImageView>
                        <Button fx:id="arrowBack" maxHeight="30.0" maxWidth="30.0" minHeight="30.0" minWidth="30.0" mnemonicParsing="false" onAction="#backFromHist" opacity="0.0" prefHeight="30.0" prefWidth="30.0" />
                     </children>
                     <GridPane.margin>
                        <Insets left="20.0" />
                     </GridPane.margin>
                  </Group>
               </children>
            </GridPane>
         </children>
      </GridPane>
   </children>
</AnchorPane>
